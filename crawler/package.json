{
  "name": "watch-twitter-likes",
  "version": "3.0.0",
  "description": "Watch Twitter user likes and notify to discord.",
  "main": "dist/main.js",
  "repository": "https://github.com/tomacheese/watch-twitter-likes",
  "author": "Tomachi",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register src/main.ts",
    "generate-schema": "typescript-json-schema --required tsconfig.json WTLConfiguration -o schema/Configuration.json",
    "lint": "run-p -c lint:prettier lint:eslint lint:tsc",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:tsc": "tsc",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src"
  },
  "devDependencies": {
    "@types/node": "18.14.0",
    "@typescript-eslint/eslint-plugin": "5.52.0",
    "@typescript-eslint/parser": "5.52.0",
    "cycle": "1.0.3",
    "discord.js": "14.7.1",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-config-standard": "17.0.0",
    "eslint-plugin-import": "2.27.5",
    "eslint-plugin-n": "15.6.1",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "45.0.2",
    "logform": "2.5.1",
    "mysql2": "3.1.2",
    "otplib": "12.0.1",
    "prettier": "2.8.4",
    "puppeteer-core": "19.7.1",
    "ts-node": "10.9.1",
    "ts-node-dev": "2.0.0",
    "twitter-api-v2": "1.14.1",
    "twitter-d": "0.6.0",
    "typeorm": "0.3.12",
    "typeorm-naming-strategies": "4.1.0",
    "typescript": "4.9.5",
    "typescript-json-schema": "0.55.0",
    "winston": "3.8.2",
    "winston-daily-rotate-file": "4.7.1",
    "yarn-run-all": "3.1.1"
  },
  "dependencies": {
    "@fastify/cookie": "8.3.0",
    "@fastify/cors": "8.2.0",
    "@fastify/session": "10.1.1",
    "fastify": "4.13.0"
  }
}
