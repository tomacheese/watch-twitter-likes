{
  "name": "watch-twitter-likes",
  "version": "4.0.0",
  "description": "Watch Twitter user likes and notify to discord.",
  "license": "MIT",
  "author": "Tomachi",
  "private": true,
  "main": "dist/main.js",
  "repository": "https://github.com/tomacheese/watch-twitter-likes",
  "scripts": {
    "start": "ts-node -r tsconfig-paths/register src/main.ts",
    "dev": "ts-node-dev -r tsconfig-paths/register src/main.ts",
    "generate-schema": "typescript-json-schema --required tsconfig.json WTLConfiguration -o schema/Configuration.json",
    "lint": "run-p -c lint:prettier lint:eslint lint:tsc",
    "lint:prettier": "prettier --check src",
    "lint:eslint": "eslint . --ext ts,tsx",
    "lint:tsc": "tsc",
    "fix": "run-s fix:prettier fix:eslint",
    "fix:eslint": "eslint . --ext ts,tsx --fix",
    "fix:prettier": "prettier --write src",
    "package": "run-s clean compile packing",
    "packing": "ncc build ./dist/main.js -o output/ -m",
    "compile": "tsc -p .",
    "clean": "rimraf dist output"
  },
  "dependencies": {
    "@fastify/cookie": "9.3.1",
    "@fastify/cors": "9.0.1",
    "@fastify/session": "10.7.0",
    "fastify": "4.26.1"
  },
  "devDependencies": {
    "@book000/node-utils": "1.12.63",
    "@book000/twitterts": "0.59.83",
    "@types/node": "20.11.19",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "@vercel/ncc": "0.38.1",
    "axios": "1.6.7",
    "discord.js": "14.14.1",
    "eslint": "8.56.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard": "17.1.0",
    "eslint-plugin-import": "2.29.1",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-unicorn": "51.0.1",
    "mysql2": "3.9.1",
    "prettier": "3.2.5",
    "ts-node": "10.9.2",
    "ts-node-dev": "2.0.0",
    "twitter-api-v2": "1.16.0",
    "twitter-d": "0.6.0",
    "typeorm": "0.3.20",
    "typeorm-naming-strategies": "4.1.0",
    "typescript": "5.3.3",
    "typescript-json-schema": "0.63.0",
    "yarn-run-all": "3.1.1"
  }
}